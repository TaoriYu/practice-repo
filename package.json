{
  "name": "template-app",
  "scripts": {
    "dev": "env NODE_ENV=development next",
    "build": "next build",
    "build:static": "gulp --gulpfile ./semantic/gulpfile.js build",
    "start": "next start",
    "lint": "tslint --project ./tsconfig.json",
    "lint:staged": "lint-staged",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:w": "jest --watch",
    "test:c": "node ./core/tests/runners/coverage.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm test && npm run typecheck"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint"
    ]
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "axios": "^0.18.0",
    "class-transformer": "^0.2.0",
    "classnames": "^2.2.6",
    "debug": "^4.1.0",
    "inversify": "^5.0.1",
    "lodash": "^4.17.11",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "mobx-react-devtools": "^6.0.3",
    "next": "^7.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "reflect-metadata": "^0.1.13",
    "semantic-ui-react": "^0.83.0",
    "ts-loader": "^5.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/debug": "0.0.31",
    "@types/enzyme": "^3.1.16",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/faker": "^4.1.5",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.119",
    "@types/next": "^7.0.5",
    "@types/react": "^16.7.13",
    "commitlint": "^7.2.1",
    "cz-customizable": "^5.3.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "faker": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^0.5.0",
    "husky": "^1.2.0",
    "hygen": "^1.6.2",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "opn": "^5.4.0",
    "semantic-ui": "^2.4.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  }
}
