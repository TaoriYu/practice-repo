{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "align": [
      true,
      "parameters",
      "statements",
      "members"
    ],
    "file-name-casing": [
      true,
      {".tsx": "pascal-case", ".ts": "camel-case", ".*": "camel-case"}
    ],
    "await-promise": true,
    "ban": false,
    "ban-types": [
      true,
      ["Object", "use {} or object instead"],
      ["String"],
      ["Boolean"]
    ],
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "curly": true,
    "eofline": true,
    "forin": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "interface-name": true,
    "no-boolean-literal-compare": true,
    "no-redundant-jsdoc": true,
    "one-variable-per-declaration": true,
    "prefer-switch": [true, {"min-cases": 2}],
    "return-undefined": true,
    "jsdoc-format": true,
    "label-position": true,
    "max-classes-per-file": [true, 1, "exclude-class-expressions"],
    "max-line-length": [
      true,
      {
        "limit": 100,
        "ignore-pattern": "^import |^export {(.*?)}|\\s\\* |(export)? class [a-zA-Z]+(<.*>)? implements |//|(export )?function [a-zA-Z]+<([a-zA-z]+ extends .*)*,?>"
      }
    ],
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "member-access": [true,
      "check-accessor",
      "check-constructor",
      "check-parameter-property"
    ],
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment":true,
    "no-duplicate-super": true,
    "no-duplicate-imports": true,
    "no-consecutive-blank-lines": true,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace",
      "log"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-floating-promises": true,
    "no-for-in-array": true,
    "no-null-keyword": true,
    "no-return-await": true,
    "no-string-throw": true,
    "no-shadowed-variable": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-internal-module": true,
    "no-require-imports": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "object-literal-key-quotes": false,
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": "always",
        "esSpecCompliant": true
      }
    ],
    "no-trailing-whitespace": true,
    "prefer-const": true,
    "no-unused-expression": true,
    "unnecessary-constructor": true,
    "use-isnan": true,
    "cyclomatic-complexity": [true, 10],
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "no-var-requires": false,
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace",
      "check-finally"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "radix": true,
    "type-literal-delimiter": true,
    "semicolon": [
      true,
      "always"
    ],
    "switch-default": true,
    "no-default-export": true,
    "triple-equals": [
      true
    ],
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "use-strict": [
      false,
      "check-module",
      "check-function"
    ],
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock",
      "check-rest-spread"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "jsx-boolean-value": ["never"],
    "jsx-curly-spacing": ["always"],
    "jsx-equals-spacing": ["never"],
    "jsx-key": true,
    "jsx-no-bind": true,
    "jsx-no-lambda": true,
    "jsx-no-multiline-js": false,
    "jsx-self-close": true,
    "jsx-space-before-trailing-slash": true,
    "jsx-wrap-multiline": true,
    "jsx-alignment": true,
    "no-parameter-reassignment": true,
    "deprecation": true,
    "newline-before-return": true,
    "callable-types": false,
    "object-curly-spacing": [
      true,
      "always",
      { "arraysInObjects": false, "objectsInObjects": true }
    ],
    "space-in-parens": [true, "never"]
  },
  "rulesDirectory": []
}
