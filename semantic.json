{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../static",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "../static/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": true,
  "rtl": false,
  "version": "2.4.2",
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "icon",
    "image",
    "input",
    "list",
    "segment",
    "grid",
    "menu"
  ]
}